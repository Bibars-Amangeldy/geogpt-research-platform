# Environment Configuration
# Copy this file to .env and fill in your API keys

# LLM Provider API Keys (at least one required)
OPENAI_API_KEY=your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here
GOOGLE_API_KEY=your-google-api-key-here

# Default LLM Provider (openai, anthropic, google, ollama)
DEFAULT_LLM_PROVIDER=openai
DEFAULT_LLM_MODEL=gpt-4o

# NASA Earthdata (optional - for NASA datasets)
EARTHDATA_USERNAME=your-earthdata-username
EARTHDATA_PASSWORD=your-earthdata-password

# Planetary Computer (optional - auto-authenticated for public data)
PLANETARY_COMPUTER_API_KEY=

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=true

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
